{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "TenantId": "YOUR_TENANT_ID",
    "ClientId": "YOUR_CLIENT_ID",
    "Audience": "api://huminex-api",
    "ValidAudiences": [
      "api://huminex-api",
      "YOUR_CLIENT_ID"
    ]
  },
  "Postgres": {
    "ConnectionString": "Host=localhost;Port=5432;Database=huminex;Username=postgres;Password=postgres",
    "ApplyMigrationsOnStartup": false
  },
  "Redis": {
    "ConnectionString": "localhost:6379"
  },
  "ServiceBus": {
    "ConnectionString": "",
    "FullyQualifiedNamespace": "",
    "EventsTopicName": "huminex-business-events"
  },
  "AzureStorage": {
    "BlobConnectionString": "UseDevelopmentStorage=true",
    "BlobServiceUri": "",
    "InterviewArtifactsContainer": "openhuman-artifacts",
    "PayrollDocumentsContainer": "payroll-documents"
  },
  "AzureAppConfiguration": {
    "Endpoint": ""
  },
  "InternalAdmin": {
    "Email": "originxlabs@gmail.com"
  },
  "AcsEmail": {
    "ConnectionString": "endpoint=https://your-acs.unitedstates.communication.azure.com/;accesskey=YOUR_KEY",
    "SenderAddress": "noreply@gethuminex.com"
  },
  "DevSecurity": {
    "EnableHeaderIdentityFallback": false,
    "FallbackTenantId": "11111111-1111-1111-1111-111111111111",
    "FallbackUserId": "22222222-2222-2222-2222-222222222222",
    "FallbackUserEmail": "admin@gethuminex.com",
    "FallbackRole": "admin",
    "FallbackPermissions": [
      "org.read",
      "org.write",
      "workforce.portal-access.write",
      "payroll.read",
      "payroll.write",
      "rbac.read",
      "rbac.write",
      "user.read.self",
      "user.roles.write"
    ]
  },
  "FeatureManagement": {
    "OpenHumanInterviewStudio": false,
    "PayrollV2": false
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.ApplicationInsights"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithEnvironmentName",
      "WithProcessId",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "ApplicationInsights",
        "Args": {
          "connectionString": "",
          "telemetryConverter": "Serilog.Sinks.ApplicationInsights.TelemetryConverters.TraceTelemetryConverter, Serilog.Sinks.ApplicationInsights"
        }
      }
    ]
  },
  "OpenTelemetry": {
    "OtlpEndpoint": "http://localhost:4317"
  }
}
